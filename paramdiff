#/bin/bash

#load my aliases
. ~/.bash_aliases

set -e

clean () {
  sed  "s/.m...m//" | grep -v "@" | grep -v "ottano_sim.parm" | grep -v "index" | sed "s/\.000000$//"
}
filt_com_cha () {
  grep -v 'BARO1_GND_PRESS\|STAT_\|MOT_THST_HOVER\|INS_GYR[12]_CALTEMP\|INS_GYR[O2]O*FFS\|COMPASS_DEC\|.SR._'
}


git diff | clean | filt_com_cha > ~/junkcode/tmp/bh_log.txt
cd ~/junkcode
python3 ap/param-combine.py > tmp/bh_comb_log.txt
cat tmp/bh_comb_log.txt
